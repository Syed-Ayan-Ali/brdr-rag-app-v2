# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google AI Studio API Key for Gemini
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key

# Alternative: OpenAI API Key (for Qwen models via OpenAI-compatible endpoint)
OPENAI_API_KEY=your_openai_api_key

# Alternative: Qwen Model Configuration (if using direct API)
QWEN_API_URL=your_qwen_api_endpoint
QWEN_API_KEY=your_qwen_api_key

# ETL Configuration
LLAMA_CLOUD_API_KEY=your_llama_cloud_api_key_optional
BRDR_API_URL=https://brdr.hkma.gov.hk/restapi/doc-search

# Embedding Configuration
EMBEDDING_MODEL=Xenova/all-MiniLM-L6-v2
EMBEDDING_DIMENSION=384

# Database Configuration
DB_POOL_SIZE=10
DB_CONNECTION_TIMEOUT=30000

# Application Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
